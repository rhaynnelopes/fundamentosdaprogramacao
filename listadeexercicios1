public class Lista1Exercicio2{
    public static void main(String args[]){
        
        double num, valor;
        num = 3;
        valor = 7;
        
        double expressao = Math.sqrt(num + 2*valor + Math.pow(valor,3)) / (num+3);
        
        System.out.println(expressao);
        
        double expressao2 = (3 + (Math.sqrt((num-valor) + num + 2 * valor + valor)))/4;
    }
}
